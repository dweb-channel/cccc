{
  "name": "cccc-sdk",
  "version": "0.1.0",
  "description": "CCCC Client SDK for Node.js - IPC client for CCCC daemon",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "npm run build && node --test dist/tests/*.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "cccc",
    "agent",
    "sdk",
    "ipc",
    "daemon",
    "ai-agent"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/types",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aspect-build/cccc.git",
    "directory": "src/sdk/ts"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
